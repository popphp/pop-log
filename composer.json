{
    "name": "popphp/pop-log",
    "description": "Pop Log Component for Pop PHP Framework",
    "keywords": [
        "log",
        "logging",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0.0",
        "popphp/pop-db": "^5.0.0",
        "popphp/pop-http": "^3.5.0",
        "popphp/pop-mail": "^3.5.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Log\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2.x-dev"
        }
    }
}
